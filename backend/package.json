{
  "name": "ship-tracking-backend",
  "version": "0.0.2",
  "description": "Backend API for Ship Tracking System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "build": "prisma generate && npx tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "ship",
    "tracking",
    "logistics"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fontsource/sarabun": "^5.2.8",
    "@line/bot-sdk": "^10.4.0",
    "@prisma/client": "^6.18.0",
    "@supabase/supabase-js": "^2.78.0",
    "@types/pdfkit": "^0.17.3",
    "airtable": "^0.12.2",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.2",
    "nodemon": "^3.1.10",
    "passport": "^0.7.0",
    "passport-line": "^0.0.4",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "prisma": "^6.18.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.9.2",
    "@types/passport": "^1.0.17",
    "@types/passport-line": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.39.0",
    "prettier": "^3.6.2"
  }
}
